{
  "name": "vetor-core-backend",
  "version": "1.0.0",
  "description": "Vetor Core Backend - Central de Performance",
  "main": "dist/server.js",
  "type": "module",
  "engines": {
    "bun": ">=1.0.0"
  },
  "scripts": {
    "dev": "bun --watch src/server.ts",
    "build": "bun build src/server.ts --outdir dist --target bun",
    "start": "bun dist/server.js",
    "db:generate": "bunx drizzle-kit generate",
    "db:migrate": "bunx drizzle-kit migrate",
    "db:push": "bunx drizzle-kit push",
    "db:studio": "bunx drizzle-kit studio"
  },
  "keywords": [
    "fastify",
    "typescript",
    "vetor",
    "real-estate"
  ],
  "author": "Vetor",
  "license": "MIT",
  "dependencies": {
    "@fastify/cors": "^11.2.0",
    "@fastify/jwt": "^9.1.0",
    "@neondatabase/serverless": "^0.9.5",
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.4.7",
    "drizzle-orm": "^0.36.4",
    "fastify": "^5.2.0",
    "node-cron": "^4.2.1",
    "pg": "^8.13.1",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^22.10.5",
    "@types/pg": "^8.11.10",
    "drizzle-kit": "^0.31.9",
    "typescript": "^5.7.3"
  }
}
